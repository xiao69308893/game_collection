{
  "name": "game-collection",
  "version": "1.0.0",
  "description": "多平台游戏集合应用",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "tauri:build:all": "tauri build --target all",
    "cap": "cap",
    "cap:add:android": "cap add android",
    "cap:add:ios": "cap add ios",
    "cap:sync": "cap sync",
    "cap:run:android": "cap run android",
    "cap:run:ios": "cap run ios",
    "cap:build:android": "npm run build && cap sync android && cap build android",
    "cap:build:ios": "npm run build && cap sync ios && cap build ios",
    "cap:copy": "cap copy",
    "cap:open:android": "cap open android",
    "cap:open:ios": "cap open ios",
    "pwa:dev": "vite --mode pwa",
    "pwa:build": "vue-tsc --noEmit && vite build --mode pwa",
    "pwa:preview": "vite preview --mode pwa",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:linux": "npm run build && electron-builder --linux",
    "miniprogram:dev": "uni build --watch -p mp-weixin",
    "miniprogram:build": "uni build -p mp-weixin",
    "miniprogram:build:alipay": "uni build -p mp-alipay",
    "miniprogram:build:baidu": "uni build -p mp-baidu",
    "miniprogram:build:toutiao": "uni build -p mp-toutiao",
    "build:all": "npm run build && npm run tauri:build && npm run cap:build:android && npm run cap:build:ios && npm run electron:build",
    "clean": "rimraf dist dist-electron node_modules/.vite",
    "reinstall": "cnpm run clean && cnpm install"
  },
  "dependencies": {
    "@capacitor/app": "^5.0.6",
    "@capacitor/core": "^5.7.0",
    "@capacitor/device": "^5.0.6",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/local-notifications": "^5.0.6",
    "@capacitor/network": "^5.0.6",
    "@capacitor/push-notifications": "^5.1.0",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@capacitor/storage": "^1.2.5",
    "electron": "^36.4.0",
    "phaser": "^3.70.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.1",
    "vue-router": "^4.3.0",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@apideck/better-ajv-errors": "latest",
    "@capacitor/android": "^5.7.0",
    "@capacitor/cli": "^5.7.0",
    "@capacitor/ios": "^5.7.0",
    "@tauri-apps/api": "^1.5.3",
    "@tauri-apps/cli": "^1.5.9",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "concurrently": "^8.2.2",
    "electron": "^36.4.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "sass": "^1.71.1",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vite-plugin-pwa": "latest",
    "vue-tsc": "^2.0.6",
    "wait-on": "^7.2.0",
    "workbox-build": "latest",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/game-collection.git"
  },
  "keywords": [
    "vue3",
    "typescript",
    "game",
    "tauri",
    "capacitor",
    "electron",
    "miniprogram",
    "pwa",
    "multiplatform"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT"
}
